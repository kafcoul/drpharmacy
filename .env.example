APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ===========================================
# CinetPay Payment Gateway Configuration
# Documentation: https://docs.cinetpay.com
# ===========================================
CINETPAY_API_KEY=your_cinetpay_api_key
CINETPAY_SITE_ID=your_cinetpay_site_id
CINETPAY_SECRET_KEY=your_cinetpay_secret_key
CINETPAY_BASE_URL=https://api-checkout.cinetpay.com/v2
CINETPAY_RETURN_URL=http://localhost/payment/return

# ===========================================
# JEKO Payment Gateway Configuration
# Documentation: https://cockpit.jeko.africa → Settings → Webhook & API
# ===========================================
JEKO_API_URL=https://api.jeko.africa
JEKO_API_KEY=your_jeko_api_key_here
JEKO_API_KEY_ID=your_jeko_api_key_id_here
JEKO_STORE_ID=your_jeko_store_id_here
JEKO_WEBHOOK_SECRET=your_jeko_webhook_secret_here
# SECURITY: IPs autorisées pour les webhooks JEKO (séparées par des virgules)
# Demandez à JEKO leurs IPs de webhook pour la production
# Laisser vide en dev uniquement - EN PRODUCTION, les webhooks seront REJETÉS si vide
JEKO_WEBHOOK_IPS=

# ===========================================
# Firebase Cloud Messaging (FCM) API V1
# ===========================================
# Téléchargez votre fichier de compte de service depuis:
# Firebase Console > Paramètres > Comptes de service > Générer une nouvelle clé privée
# Placez le fichier JSON dans storage/app/firebase-credentials.json
FIREBASE_CREDENTIALS=storage/app/firebase-credentials.json
